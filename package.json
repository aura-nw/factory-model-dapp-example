  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "apps",
    "testRegex": ".*\\.spec\\.ts$",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/migrations/"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1",
      "^artaverse-submodule/(.*)$": "<rootDir>/../artaverse-submodule/$1"
